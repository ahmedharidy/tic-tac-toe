{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-plusplus": "off",
    "arrow-parens": "off",
    "indent": ["error", 4],
    "consistent-return": "off"
  }
}
